{
    "name": "cakedc/cakephp-db-test",
    "description": "DbTest plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": ">=3.5.0 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\DbTest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\DbTest\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
